{
  "system": {
    "name": "MetaRing",
    "executionEnvironment": "DEVELOPMENT"
  },
  "ext": {
    "spring": {
      "instances": 1,
      "webServices": {
        "accessPointName": "call",
        "sockjs": true
      },
      "rest": {
        "applicationName": "/hello"
      },
      "data": {
        "mongodb": {
          "host": "localhost",
          "port": 27017,
          "database": "metaringPrototype"
        }
      }
    }
  },
  "sensitivedatapersistence": {
    "paramsToTrasform": [
      "username",
      "name",
      "surname",
      "email",
      "ipAddress",
      "accesses.ipAddress",
      "registrationIp"
    ]
  },
  "auth": {
       "tokenTimeExpiringMinutes": 20,
       "authTokenPreExpirationMinutes": 3
  }
}